{
  "name": "xtal-editor",
  "version": "0.0.13",
  "keywords": [
    "Web Components",
    "Custom Elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "src/xtal-editor.js",
  "module": "src/xtal-editor.js",
  "exports": {
    ".": {
      "import": "src/xtal-editor.js"
    }
  },
  "files": [
    "*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "xtal-element": "0.0.314",
    "trans-render": "0.0.269"
  },
  "devDependencies": {
    "es-dev-server": "1.57.8",
    "es-module-shims": "0.7.1",
    "@rollup/plugin-node-resolve": "10.0.0",
    "xtal-test": "0.0.48",
    "@skypack/package-check": "0.2.2"
  },
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "es-dev-server --app-index demo/dev.html --node-resolve --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "wca analyze src/xtal-editor-example1.js --outFile custom-elements.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme"
}
