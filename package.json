{
  "name": "xtal-editor",
  "version": "0.0.180",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "xtal-editor.js",
  "module": "xtal-editor.js",
  "exports": {
    ".": "./xtal-editor.js",
    "./xtal-editor.js": "./xtal-editor.js",
    "./xtal-editor-field.js": "./xtal-editor-field.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "@power-elements/json-viewer": "2.1.1",
    "be-active": "0.0.37",
    "be-adopted": "0.0.4",
    "be-channeling": "0.0.21",
    "be-decorated": "0.0.133",
    "be-definitive": "0.0.59",
    "be-deslotted": "0.0.34",
    "be-importing": "0.0.30",
    "be-hive": "0.0.56",
    "be-lazy": "0.0.14",
    "be-loaded": "0.0.70",
    "be-noticed": "0.0.67",
    "be-observant": "0.0.132",
    "be-repeated": "0.0.80",
    "be-switched": "0.0.75",
    "be-transformative": "0.0.44",
    "may-it-be": "0.0.164",
    "trans-render": "0.0.574",
    "xtal-element": "0.0.513",
    "xtal-side-nav": "0.0.87",
    "xtal-tree": "0.0.162",
    "xtal-vlist": "0.0.73"
  },
  "devDependencies": {
    "@playwright/test": "1.25.0",
    "may-it-serve": "0.0.2",
    "@custom-elements-manifest/analyzer": "0.6.4",
    "@skypack/package-check": "0.2.2",
    "be-persistent": "0.0.26",
    "npm-watch": "0.11.0",
    "xtal-shell": "0.0.27",
    "typescript": "4.6.3"
  },
  "watch": {
    "build-xe-html": "*-config.mjs",
    "build-xe-js": "*-config.mjs",
    "build-manifest": "types.d.ts"
  },
  "scripts": {
    "serve": "node node_modules/may-it-serve",
    "test": "playwright test",
    "start": "web-dev-server --node-resolve --app-index demo/dev.html  --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze",
    "build-xe-html": "node xe-config.mjs -html > xtal-editor.html",
    "build-xe-js": "node xe-config.mjs -js > xtal-editor.js",
    "build-manifest": "node doc.mjs > custom-elements.json",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme",
  "customElements": "custom-elements.json"
}
