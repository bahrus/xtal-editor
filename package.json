{
  "name": "xtal-editor",
  "version": "0.0.127",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "xtal-editor.js",
  "module": "xtal-editor.js",
  "exports": {
    ".": "./xtal-editor.js",
    "./xtal-editor.js": "./xtal-editor.js",
    "./xtal-editor-field.js": "./xtal-editor-field.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "@power-elements/json-viewer": "2.1.1",
    "be-active": "0.0.17",
    "be-decorated": "0.0.70",
    "be-definitive": "0.0.28",
    "be-deslotted": "0.0.16",
    "be-hive": "0.0.20",
    "be-intersectional": "0.0.12",
    "be-loaded": "0.0.42",
    "be-noticed": "0.0.47",
    "be-observant": "0.0.88",
    "be-repeated": "0.0.40",
    "be-switched": "0.0.56",
    "be-transformative": "0.0.25",
    "may-it-be": "0.0.103",
    "trans-render": "0.0.449",
    "xtal-element": "0.0.462",
    "xtal-side-nav": "0.0.58"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.5.7",
    "@skypack/package-check": "0.2.2",
    "be-importing": "0.0.1",
    "be-persistent": "0.0.8",
    "npm-watch": "0.11.0",
    "xtal-shell": "0.0.25",
    "xtal-test": "0.0.55",
    "typescript": "^4.6.0-dev.20220113"
  },
  "watch": {
    "build-xe-html": "*-config.mjs",
    "build-xe-js": "*-config.mjs",
    "build-xef": "*-config.mjs"
  },
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "node test/test-all.js",
    "start": "web-dev-server --node-resolve --app-index demo/dev.html  --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze",
    "build-xe-html": "node xe-config.mjs -html > xtal-editor.html",
    "build-xe-js": "node xe-config.mjs -js > xtal-editor.js",
    "build-xef": "node xef-config.mjs > xef-config.json",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme",
  "customElements": "custom-elements.json"
}
