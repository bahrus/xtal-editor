{
  "name": "xtal-editor",
  "version": "0.0.78",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "src/xtal-editor.js",
  "module": "src/xtal-editor.js",
  "exports": {
    ".": "./src/xtal-editor.js",
    "./xtal-editor.js": "./src/xtal-editor.js",
    "./src/xtal-editor.js": "./src/xtal-editor.js"
  },
  "files": [
    "src/*",
    "custom-elements.json",
    "types.d.ts"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-observant": "0.0.15",
    "be-noticed": "0.0.17",
    "be-transformative": "0.0.1",
    "be-switched": "0.0.1",
    "be-repeated": "0.0.5",
    "ib-id": "0.0.82",
    "xtal-side-nav": "0.0.15",
    "xtal-element": "0.0.443",
    "trans-render": "0.0.419",
    "@power-elements/json-viewer": "2.1.1",
    "xtal-decor": "0.0.52"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.51",
    "@custom-elements-manifest/analyzer": "0.5.5",
    "xtal-shell": "0.0.25"
  },
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "node test/test-all.js",
    "start": "web-dev-server --node-resolve --app-index demo/dev.html  --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme",
  "customElements": "custom-elements.json"
}
