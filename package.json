{
  "name": "xtal-editor",
  "version": "0.0.111",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "xtal-editor.js",
  "module": "xtal-editor.js",
  "exports": {
    ".": "./xtal-editor.js",
    "./xtal-editor.js": "./xtal-editor.js",
    "./xtal-editor-field.js": "./xtal-editor-field.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "trans-render": "0.0.442",
    "xtal-element": "0.0.457",
    "may-it-be": "0.0.10",
    "be-active": "0.0.12",
    "be-decorated": "0.0.57",
    "be-deslotted": "0.0.14",
    "be-hive": "0.0.16",
    "be-loaded": "0.0.27",
    "be-observant": "0.0.71",
    "be-noticed": "0.0.41",
    "be-persistent": "0.0.7",
    "be-repeated": "0.0.36",
    "be-switched": "0.0.48",
    "be-transformative": "0.0.22",
    "xtal-side-nav": "0.0.47",
    "@power-elements/json-viewer": "2.1.1"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.54",
    "@custom-elements-manifest/analyzer": "0.5.7",
    "xtal-shell": "0.0.25",
    "npm-watch": "0.11.0"
  },
  "watch": {
    "build": "*-config.js"
  },
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "node test/test-all.js",
    "start": "web-dev-server --node-resolve --app-index demo/dev.html  --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze",
    "build": "copy xe-config.js xe-config.mjs && copy xef-config.js xef-config.mjs && node xe-config.mjs > xtal-editor.js && node xef-config.mjs > xef-config.json",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme",
  "customElements": "custom-elements.json"
}
