{
  "name": "xtal-editor",
  "version": "0.0.140",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Edit JSON",
  "main": "xtal-editor.js",
  "module": "xtal-editor.js",
  "exports": {
    ".": "./xtal-editor.js",
    "./xtal-editor.js": "./xtal-editor.js",
    "./xtal-editor-field.js": "./xtal-editor-field.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "@power-elements/json-viewer": "2.1.1",
    "be-active": "0.0.27",
    "be-channeling": "0.0.7",
    "be-decorated": "0.0.91",
    "be-definitive": "0.0.42",
    "be-deslotted": "0.0.24",
    "be-intersectional": "0.0.44",
    "be-hive": "0.0.36",
    "be-lazy": "0.0.2",
    "be-loaded": "0.0.51",
    "be-noticed": "0.0.57",
    "be-observant": "0.0.103",
    "be-repeated": "0.0.65",
    "be-switched": "0.0.64",
    "be-transformative": "0.0.33",
    "may-it-be": "0.0.145",
    "trans-render": "0.0.516",
    "xtal-element": "0.0.485",
    "xtal-side-nav": "0.0.70",
    "xtal-tree": "0.0.121",
    "xtal-vlist": "0.0.53"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.5.7",
    "@skypack/package-check": "0.2.2",
    "be-importing": "0.0.22",
    "be-persistent": "0.0.13",
    "npm-watch": "0.11.0",
    "xtal-shell": "0.0.25",
    "xtal-test": "0.0.56",
    "typescript": "4.6.2"
  },
  "watch": {
    "build-xe-html": "*-config.mjs",
    "build-xe-js": "*-config.mjs",
    "build-xe-html2": "*-config2.mjs",
    "build-xe-js2": "*-config2.mjs",
    "build-xef": "*-config.mjs"
  },
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "node test/test-all.js",
    "start": "web-dev-server --node-resolve --app-index demo/dev.html  --watch --open",
    "bundle": "rollup --config",
    "compress": "node minify",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze",
    "build-xe-html": "node xe-config.mjs -html > xtal-editor.html",
    "build-xe-js": "node xe-config.mjs -js > xtal-editor.js",
    "build-xe-html2": "node xe-config2.mjs -html > xtal-editor2.html",
    "build-xe-js2": "node xe-config2.mjs -js > xtal-editor2.js",
    "build-xef": "node xef-config.mjs > xef-config.json",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-editor.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-editor/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-editor#readme",
  "customElements": "custom-elements.json"
}
